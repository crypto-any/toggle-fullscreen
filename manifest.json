{
  "manifest_version": 2,
  "name": "Tela Cheia",
  "description": "Alterna entre tela cheia e tela normal ao clicar no ícone da extensão.",
  "version": "1.0",
  "background": {
    "scripts": ["toggle_fullscreen.js"]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Alternar tela cheia"
  },
  "permissions": [
    "activeTab"
  ]
}
